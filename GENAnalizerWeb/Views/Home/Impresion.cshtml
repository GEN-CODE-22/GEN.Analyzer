<script>
    var servidor;
    var geocercasP = [];
    var geocercasC = [];

   
    var planta = null;
    var compañia = null;
    
  

    $("#scrollH").scrollLeft(300);
    $(document).ready(function () {
        servidor = document.getElementById('serv').innerText;
        planta = $("#hiddenPla").val();
        compañia = $("#hiddenCia").val();
        if (localStorage.getItem("servidor")==null ||localStorage.getItem("servidor")!=servidor ) 
        {
            localStorage.setItem("servidor", servidor);
            //getGeocercas();
    
        }

        if ((localStorage.getItem("geocercasP") == null || localStorage.getItem("geocercasC") == null) || (localStorage.getItem("cia") != compañia || localStorage.getItem("pla") != planta)) {
            localStorage.removeItem("geocercasP");
            localStorage.removeItem("geocercasC");
            //localStorage.setItem("cia", compañia);
            //localStorage.setItem("pla", planta);
            getGeocercas();
            
        }

        mostrarSegmentos();
      
        


    });
  
    function selectSecmento(id) {
       
        document.getElementById('divPersonal').style.display = "none";
        document.getElementById('divGerente').style.display = "none";
        document.getElementById('divCrdCob').style.display = "none";
        document.getElementById('divAdmin').style.display = "none";
        switch (id.id) {
            case "seccPers":
              //  document.getElementById('accordionSidebar').style.background = "#e74a3b"
                document.getElementById('divPersonal').style.display = "inline";
                break;
            case "seccGerente":
              //  document.getElementById('accordionSidebar').style.background = "#4e73df"
                document.getElementById('divGerente').style.display = "inline";
                break;
            case "seccCrdCob":
              //  document.getElementById('accordionSidebar').style.background = "#36b9cc"
                document.getElementById('divCrdCob').style.display = "inline";
                break;
            case "seccAdmin":
              //  document.getElementById('accordionSidebar').style.background = "#000000"
                document.getElementById('divAdmin').style.display = "inline";
                break;

            default:

        }

    }
    function mostrarSegmentos() // muestra la accesibilidad de los segmentos de acuedo al tipo de usuario
    {
        var rol = document.getElementById('rol').innerText;



        switch (rol) {
            case"Personal":
                //document.getElementById('accordionSidebar').style.background = "#e74a3b"
                document.getElementById('seccPers').style.display = "inline";
                break;
            case "Gerente":
                if (servidor != "Admin") {
                   // document.getElementById('accordionSidebar').style.background = "#4e73df"
                    document.getElementById('seccGerente').style.display = "inline";

                }
                else {
                   // document.getElementById('accordionSidebar').style.background = "#000000"
                    
                }
               

                break;
            case "CredCob":
                //document.getElementById('accordionSidebar').style.background = "#36b9cc"
                document.getElementById('seccCrdCob').style.display = "inline";

                break;
            case "Administrador":
                //document.getElementById('accordionSidebar').style.background = "#000000"
                document.getElementById('seccPers').style.display = "inline";
                document.getElementById('seccAdmin').style.display = "inline";
                document.getElementById('seccCrdCob').style.display = "inline";
                document.getElementById('seccGerente').style.display = "inline";
                break;

        }
        if (localStorage.getItem("empresa") == 'PLUS')
        {
            var x = document.getElementsByClassName("GEN");
            var i;
            for (i = 0; i < x.length; i++) {
                x[i].style.display = 'none';
            }
        }
           
   
    }

    function getGeocercas()
    {
        $.ajax({
            type: "POST",
            url: "/Home/getGeocercas/",
            data: { paramSrv: document.getElementById('serv').innerHTML, paramCia: localStorage.getItem('cia') , paramPla: localStorage.getItem('pla') },
            datatype: "json",
            success: function (data) {

                if (data.length > 0) {

                    for (var i = 0; i < data.length; i++) 
                    {
                        if(data[i].nom_geo.substring(0, 1) == 'M')
                            geocercasP.push(data[i]);
                        if(data[i].nom_geo.substring(0,1)=='C')
                            geocercasC.push(data[i]);
                    }
                    localStorage.setItem('geocercaP', JSON.stringify(geocercasP));
                    sessionStorage.setItem("geocercaC", JSON.stringify(geocercasC));
                    //localStorage.setItem('geocercaC', JSON.stringify(geocercasC));
                    
                   
                    alert("geocercas cargadas");
                }
                else {
                    
                    alert("No existe información de geocercas en el servidor ingresado");
                }
            },
            error: function (request, status, error) {

                alert(request.responseText);
            }
        });
    }

